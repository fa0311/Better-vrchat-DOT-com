{
    "manifest_version": 2,
    "name": "VRChat+",
    "version": "1.2.0",
    "description": "VRChat公式サイトをちょっと便利にする拡張機能",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://vrchat.com/home/*",
                "https://www.vrchat.com/home/*"
            ],
            "js": [
                "vrchat_plus.js"
            ]
        }
    ]
}